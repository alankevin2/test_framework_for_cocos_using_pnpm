{ 
    "compilerOptions": { 
        "target": "esnext",
        "module": "esnext", 
        "lib": [
            "esnext", 
            "dom", 
            "dom.iterable", 
        ], 
        "allowJs": true,
        "removeComments": true,
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": false,
        "noUnusedLocals": true,
        "moduleResolution": "node",
        "baseUrl": "./",
        "resolveJsonModule": true,
        "esModuleInterop": true
    }, "include": [ 
        "src/*.ts", 
        "src/types.ts"
    ]
}